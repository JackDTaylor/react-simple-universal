{
  "name": "universal-react-boilerplate",
  "version": "3.0.0",
  "description": "A new Webpack boilerplate with hot reloading React components, and error handling on module and component level.",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "NODE_PATH=source babel-node devServer.js",
    "lint": "eslint source"
  },
  "engines": {
    "node": ">=4"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack",
    "babel",
    "react-transform"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:ericelliott/isomorphic-express-boilerplate.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericelliott/isomorphic-express-boilerplate/issues"
  },
  "homepage": "https://github.com/ericelliott/isomorphic-express-boilerplate",
  "dependencies": {
    "react-simple-universal": "0.4.0",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-router": "2.0.1",
    "redux": "3.3.1",
    "redux-thunk": "2.0.1"
  },
  "devDependencies": {
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
    "babel-loader": "6.2.4",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-runtime": "6.6.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "cross-env": "1.0.7",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.2",
    "nodemon": "^1.9.1",
    "react-transform-catch-errors": "1.0.2",
    "react-transform-hmr": "1.0.4",
    "redbox-react": "1.2.2",
    "rimraf": "2.5.2",
    "watch": "^0.17.1",
    "webpack": "1.12.14",
    "webpack-hot-middleware": "2.10.0"
  }
}
